
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  nombre String
  apellido String
  correo String
  ciudad String
  pais String
  salario Int
  edad Int
  altura Int
  peso Int
}